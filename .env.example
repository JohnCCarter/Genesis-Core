BITFINEX_API_KEY=
BITFINEX_API_SECRET=
BITFINEX_WS_API_KEY=
BITFINEX_WS_API_SECRET=
API_BASE=https://api.bitfinex.com
LOG_LEVEL=INFO

SYMBOL_MODE=test
WALLET_CAP_ENABLED=1

# Global loggnivå (högsta detaljnivå vinner om flera anges)
CORE_LOG_LEVEL=WARNING
LOG_LEVEL=INFO

# Slå av fib-diagnostiken när du vill ha tyst körning
FIB_FLOW_LOGS_ENABLED=0

GITHUB_PERSONAL_ACCESS_TOKEN=
GITHUB_TOOLSETS=all ./github-mcp-server
SUPABASE_ACCESS_TOKEN=
RENDER_TOKEN=

GENESIS_IN_PROCESS=0
GENESIS_OPTIMIZER_JSON_CACHE=1
# OBS: Låt GENESIS_FAST_HASH=0 för jämförbara och reproducerbara resultat.
# GENESIS_FAST_HASH=1 är en "fast path" för feature-cache-nyckling som kan ändra utfall (även med samma seed/period).
GENESIS_FAST_HASH=0
GENESIS_MAX_CONCURRENT=4
GENESIS_FAST_WINDOW=1
# Server/UI: kör normalt utan precompute-injektion. Backtest/optimizer i canonical mode
# enforce:ar ändå precompute via GenesisPipeline.setup_environment().
GENESIS_PRECOMPUTE_FEATURES=0
GENESIS_DISABLE_METRICS=1
GENESIS_RANDOM_SEED=42
GENESIS_HTF_EXITS=1
GENESIS_SCORE_VERSION=v2
OPTUNA_MAX_DUPLICATE_STREAK=2000

# Scoring/constraints (valfritt, men bra att vara explicit för reproducerbarhet)
# GENESIS_SCORE_VERSION=v2
GENESIS_CONSTRAINT_SOFT_PENALTY=150

# Optuna/runner (valfritt)
# GENESIS_FORCE_SHELL=0
# GENESIS_OPTUNA_CHECKPOINT_EVERY=25

# FastAPI config (valfritt)
PORT=8000
BEARER_TOKEN=

# MCP Genesis-Core (remote / ChatGPT connector)
# Använd separat port-variabel för att undvika konflikt med FastAPI:s PORT.
GENESIS_MCP_PORT=3333
GENESIS_MCP_REMOTE_SAFE=1
GENESIS_MCP_REMOTE_ULTRA_SAFE=0
GENESIS_MCP_REMOTE_TOKEN=

CONTEXT7_API_KEY=

# Context7 MCP: används för att kryptera client IP och förhindrar en startup-warning
# som annars kan bryta stdio/MCP-handshake.
CLIENT_IP_ENCRYPTION_KEY=

# Remote ops (Windows → VM orchestrator)
# SSH config alias (e.g. genesis-we) or user@host. Used by scripts/phase3_remote_orchestrate.ps1
GENESIS_SSH_TARGET=
