{
  "features": {
    "code_execution": true,
    "file_operations": true,
    "git_integration": true
  },
  "log_file": "logs/mcp_server.log",
  "log_level": "INFO",
  "security": {
    "allowed_paths": ["."],
    "blocked_patterns": [
      ".git",
      "__pycache__",
      "*.pyc",
      "node_modules",
      ".env",
      ".nonce_tracker.json",
      "dev.overrides.local.json"
    ],
    "execution_timeout_seconds": 30,
    "max_file_size_mb": 10
  },
  "server_name": "genesis-core",
  "version": "1.0.0"
}
